{{- $.Scratch.Add "index" slice -}}
{{- range $index, $element := .Data.Pages -}}
    {{- $.Scratch.Add "index" (dict "id" $index "name" $element.Name "colour" $element.Params.colours "district" $element.Params.districts "category" $element.Params.categories "link" $element.RelPermalink) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
